{
  "name": "@clive/prompts",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./sections": {
      "types": "./dist/sections/index.d.ts",
      "source": "./src/sections/index.ts",
      "default": "./dist/sections/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome check .",
    "format:fix": "biome check --write .",
    "clean": "git clean -xdf .cache .turbo dist node_modules"
  },
  "dependencies": {
    "effect": "^3.19.9"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@clive/biome-config": "*",
    "@clive/tsconfig": "*",
    "tsup": "^8.4.0",
    "typescript": "^5.9.3"
  }
}
