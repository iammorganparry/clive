{
  "extends": "@clive/tsconfig/compiled-package.json",
  "compilerOptions": {
    "emitDeclarationOnly": false,
    "outDir": "./dist",
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "noEmit": false
  },
  "include": ["src"],
  "exclude": ["node_modules", "src/__tests__", "src/__mocks__"]
}
