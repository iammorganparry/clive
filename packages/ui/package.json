      "default": "./dist/components/ai-elements/message.js"
    },
    "./components/ai-elements/prompt-input": {
      "types": "./dist/components/ai-elements/prompt-input.d.ts",
      "default": "./dist/components/ai-elements/prompt-input.js"
    },
    "./components/ai-elements/tool": {
      "types": "./dist/components/ai-elements/tool.d.ts",
      "default": "./dist/components/ai-elements/tool.js"
    },
    "./components/ai-elements/reasoning": {
      "types": "./dist/components/ai-elements/reasoning.d.ts",
      "default": "./dist/components/ai-elements/reasoning.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup && yarn build:css",
    "build:css": "tailwindcss -i src/styles.src.css -o dist/styles.css --minify",
    "dev": "tsup --watch",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "biome check .",
    "format:fix": "biome check --write .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "ui-add": "npx shadcn@latest add && biome check --write src"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "@tabler/icons-react": "^3.35.0",
