    },
    "./progress": {
      "types": "./dist/progress.d.ts",
      "default": "./dist/progress.js"
    },
    "./task": {
      "types": "./dist/task.d.ts",
      "default": "./dist/task.js"
    },
    "./switch": {
      "types": "./dist/switch.d.ts",
      "default": "./dist/switch.js"
    },
    "./input-otp": {
      "types": "./dist/input-otp.d.ts",
      "default": "./dist/input-otp.js"
    },
    "./badge": {
      "types": "./dist/badge.d.ts",
      "default": "./dist/badge.js"
    },
    "./blocks": {
      "types": "./dist/components/blocks/index.d.ts",
      "default": "./dist/components/blocks/index.js"
