    "lucide-react": "^0.562.0",
    "motion": "^12.23.26",
    "nanoid": "^5.1.6",
    "radix-ui": "^1.4.3",
    "shiki": "^3.20.0",
    "sonner": "^2.0.7",
    "streamdown": "^1.6.10",
    "tailwind-merge": "^3.4.0",
    "use-stick-to-bottom": "^1.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@clive/biome-config": "*",
    "@clive/tsconfig": "*",
    "@tailwindcss/cli": "^4.1.17",
    "@types/react": "^19.2.7",
    "react": "^19.2.0",
    "tailwindcss": "^4.1.17",
    "tsup": "^8.4.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "zod": "^4.1.13"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "zod": "^3.24.1"
  }
