{
  "name": "@clive/core",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./diff": {
      "types": "./dist/diff/index.d.ts",
      "default": "./dist/diff/index.js"
    },
    "./editor": {
      "types": "./dist/editor/index.d.ts",
      "default": "./dist/editor/index.js"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "biome check .",
    "format:fix": "biome check --write .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "effect": "^3.19.9"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@clive/biome-config": "*",
    "@clive/tsconfig": "*",
    "@vitest/ui": "^4.0.16",
    "tsup": "^8.4.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
