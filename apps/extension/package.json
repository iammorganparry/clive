{
  "name": "clive",
  "publisher": "iammorganparry",
  "displayName": "Clive - AI Test Writer",
  "description": "Your AI-powered test writer for automated testing across all languages and frameworks",
  "version": "0.0.1-dev.5ac447a.1767460002",
  "repository": {
    "type": "git",
    "url": "https://github.com/iammorganparry/clive.git"
  },
  "engines": {
    "vscode": "^1.105.0"
  },
  "enabledApiProposals": [
    "editorInsets"
  ],
  "categories": [
    "Other"
  ],
  "type": "module",
  "activationEvents": [],
  "main": "./dist/extension.cjs",
  "contributes": {
    "commands": [
      {
        "command": "clive.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "clive.showView",
        "title": "Show Clive",
        "icon": "$(beaker)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "clive",
          "title": "Clive",
          "icon": "$(beaker)"
        }
      ]
    },
    "views": {
      "clive": [
        {
          "id": "clive.mainView",
          "icon": "*",
          "name": "Clive",
          "type": "webview",
          "webviewOptions": {
            "retainContextWhenHidden": true
          }
        }
      ]
    },
    "uris": [
      {
        "scheme": "vscode",
        "pattern": "clive.auth/callback"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn package",
    "compile": "yarn check-types && yarn lint && yarn build:extension && yarn build:webview",
    "build:extension": "node esbuild.js",
    "build:webview": "vite build",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:vite": "vite build --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "yarn check-types && yarn lint && node esbuild.js --production && vite build",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn compile-tests && yarn compile && yarn lint",
    "check-types": "tsc --noEmit",
    "format": "biome check .",
    "format:fix": "biome check --write .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "test": "vscode-test",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "set-dev-version": "node scripts/set-dev-version.js",
    "install:local": "turbo build && yarn set-dev-version && rm -f *.vsix && npx @vscode/vsce package --no-dependencies --allow-missing-repository && code --install-extension $(ls -t *.vsix | head -1) --force",
    "install:cursor": "turbo build && yarn set-dev-version && rm -f *.vsix && npx @vscode/vsce package --no-dependencies --allow-missing-repository && cursor --install-extension $(ls -t *.vsix | head -1) --force",
    "package:vsix": "npx @vscode/vsce package --no-dependencies --allow-missing-repository",
    "prepare": "husky",
    "commitlint": "commitlint --edit"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "@clive/biome-config": "*",
    "@clive/tsconfig": "*",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@effect/vitest": "^0.27.0",
    "@iconify/react": "^6.0.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "@types/diff": "^8.0.0",
    "@types/jwt-decode": "^3.1.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/vscode": "^1.105.0",
    "@vitejs/plugin-react": "^5.1.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "autoprefixer": "^10.4.22",
    "esbuild": "^0.27.0",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "npm-run-all": "^4.1.5",
    "react-grab": "^0.0.94",
    "semantic-release-vsce": "^6.0.17",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.16",
    "vitest-mock-extended": "^3.1.0"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.0",
    "@ai-sdk/openai": "^3.0.0",
    "@ai-sdk/openai-compatible": "^2.0.0",
    "@ai-sdk/xai": "^3.0.1",
    "@clerk/clerk-react": "5.57.0",
    "@clive/api": "*",
    "@clive/core": "*",
    "@clive/db": "*",
    "@clive/ui": "*",
    "@clive/webview-rpc": "*",
    "@iconify-json/vscode-icons": "^1.2.37",
    "@iconify/react": "^6.0.2",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/supabase-js": "^2.89.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-query": "^5.90.12",
    "@trpc/client": "^11.7.2",
    "@vscode/codicons": "^0.0.44",
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "@xstate/react": "^6.0.0",
    "ai": "^6.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.19",
    "diff": "^8.0.2",
    "effect": "^3.19.9",
    "firecrawl-aisdk": "^0.7.2",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.562.0",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "superjson": "2.2.3",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.4.0",
    "xstate": "^5.25.0",
    "zod": "^4.1.13"
  }
}
